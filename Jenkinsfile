pipeline {
agent any 
stages {

    stage ("init") {
        step {
        echo "initializing pipeline"
        }
    }
    stage ('build') {
        step {
        echo "building the pipeline"
        }
    }
    stage ('test') {
        step {
        echo "testing it"
        }
    }
    stage ('deployment') {
        step {
        echo "deployment completed"
        }
    }
}
}